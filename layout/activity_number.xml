<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Registerphone2">
    <TextView
        android:id="@+id/Certification_register_main"
        android:layout_width="74dp"
        android:layout_height="24dp"
        android:text="@string/numer"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="74dp"
        android:layout_marginStart="142dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/Certification_register_back3"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="68dp"
        android:layout_marginStart="310dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/close"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/Certification_register_no"
        android:layout_width="269dp"
        android:layout_height="24dp"
        android:text="@string/number_go"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="167dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edit_Certification"
        android:layout_width="285dp"
        android:layout_height="22dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="28dp"
        android:layout_marginTop="208dp"
        android:maxLength="13"
        android:hint="@string/input_num"
        android:importantForAutofill="no"
        android:background="@null"
        android:inputType="numberPassword"
        tools:ignore="LabelFor,TextFields" />

    <View
        android:id="@+id/line_177"
        android:layout_width="285dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="21dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/line_177"
        />

    <TextView
        android:id="@+id/timerTextView"
        android:layout_width="34dp"
        android:layout_height="20dp"
        android:textSize="17sp"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="158dp"
        android:layout_marginTop="264dp"
        android:text="0:00" />

    <TextView
        android:id="@+id/Certification_register_no2"
        android:layout_width="157dp"
        android:layout_height="18dp"
        android:text="@string/ㄱ두ㅕㅡ"
        android:textSize="14sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:gravity="top"
        android:includeFontPadding="false"
        android:layout_marginTop="415dp"
        android:layout_marginStart="102dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/line_178"
        android:layout_width="168dp"
        android:layout_height="1dp"
        android:layout_marginTop="438dp"
        android:layout_marginStart="95dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/line_178"
        />


    <Button
        android:id="@+id/Certification_btn_ok"
        android:layout_width="328dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="456dp"
        android:background="@drawable/rectangle_01"
        android:backgroundTint="#FBEFFF"
        android:text="@string/ok"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>