<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Member_register">

    <TextView
        android:id="@+id/mwmber_main"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="@string/member"
        android:textSize="27sp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="75dp"
        android:layout_marginStart="129dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/member_back"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginTop="67dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/navigate_before"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/member_email"
        android:layout_width="253dp"
        android:layout_height="wrap_content"
        android:text="@string/sendregister"
        android:textSize="20sp"
        android:textColor="#000000"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="-3sp"
        android:includeFontPadding="false"
        android:layout_marginTop="186dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edit_register"
        android:layout_width="300dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="245dp"
        android:importantForAutofill="no"
        android:background="@null"
        android:inputType="numberPassword"
        tools:ignore="LabelFor,TextFields" />

    <View
        android:id="@+id/line_177"
        android:layout_width="327dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="19dp"
        android:layout_marginTop="267dp"
        android:background="@drawable/line_1"
        />

    <TextView
        android:id="@+id/timerTextView"
        android:layout_width="34dp"
        android:layout_height="20dp"
        android:textSize="17sp"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="163dp"
        android:layout_marginTop="296dp"
        android:text="0:00" />

    <TextView
        android:id="@+id/Certification_register_no2"
        android:layout_width="157dp"
        android:layout_height="18dp"
        android:text="@string/ㄱ두ㅕㅡ"
        android:textSize="14sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:gravity="top"
        android:includeFontPadding="false"
        android:layout_marginTop="429dp"
        android:layout_marginStart="103dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/line_178"
        android:layout_width="168dp"
        android:layout_height="1dp"
        android:layout_marginTop="449dp"
        android:layout_marginStart="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/line_178"
        />

    <Button
        android:id="@+id/register_ok"
        android:layout_width="328dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="460dp"
        android:background="@drawable/rectangle_4"
        android:text="@string/ok"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>