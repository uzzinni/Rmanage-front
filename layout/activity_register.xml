<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register">

    <TextView
        android:id="@+id/phone_register_main"
        android:layout_width="74dp"
        android:layout_height="24dp"
        android:text="@string/numer"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="74dp"
        android:layout_marginStart="142dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/phone_register_back"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="68dp"
        android:layout_marginStart="310dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/close"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/phone_register"
        android:layout_width="85dp"
        android:layout_height="15dp"
        android:text="@string/registered"
        android:textSize="12sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="129dp"
        android:layout_marginStart="21dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/phone_register_no"
        android:layout_width="212dp"
        android:layout_height="24dp"
        android:text="@string/no_phone"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="169dp"
        android:layout_marginStart="21dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/line_177"
        android:layout_width="285dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="21dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/line_177"
        />
    
    <Button
        android:id="@+id/register_btn"
        android:layout_width="328dp"
        android:layout_height="45dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="657dp"
        android:text="@string/register_phone"
        android:textSize="20sp"
        android:backgroundTint="#FBEFFF"
        android:background="@drawable/rectangle_01"/>
</androidx.constraintlayout.widget.ConstraintLayout>