<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Membership">

    <TextView
        android:id="@+id/mwmber_main"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="@string/member"
        android:textSize="27sp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="75dp"
        android:layout_marginStart="129dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/member_back"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginTop="67dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/navigate_before"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/member_email"
        android:layout_width="197dp"
        android:layout_height="wrap_content"
        android:text="@string/inputemail"
        android:textSize="20sp"
        android:textColor="#000000"
        android:letterSpacing="0.01"
        android:lineSpacingExtra="-3sp"
        android:includeFontPadding="false"
        android:layout_marginTop="186dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edit_email"
        android:layout_width="300dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="239dp"
        android:importantForAutofill="no"
        android:background="@null"
        android:inputType="textEmailAddress"
        tools:ignore="LabelFor,TextFields" />
    <TextView
        android:id="@+id/resultTextView"
        android:layout_width="250dp"
        android:layout_height="19dp"
        android:text=""
        android:textSize="16sp"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="22dp"
        android:layout_marginTop="280dp" />

    <View
        android:id="@+id/line_177"
        android:layout_width="327dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="19dp"
        android:layout_marginTop="267dp"
        android:background="@drawable/line_1"
        />

    <Button
        android:id="@+id/email_ok"
        android:layout_width="328dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="460dp"
        android:background="@drawable/rectangle_4"
        android:text="@string/ok"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>