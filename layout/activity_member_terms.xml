<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Member_terms">

    <TextView
        android:id="@+id/terms_main"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="@string/member"
        android:textSize="27sp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="76dp"
        android:layout_marginStart="129dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/terms_back"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginTop="67dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/navigate_before"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/terms_text"
        android:layout_width="287dp"
        android:layout_height="30dp"
        android:text="@string/agreeterms"
        android:textSize="16sp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="140dp"
        android:layout_marginStart="36dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="328dp"
        android:layout_height="58dp"
        android:layout_marginTop="205dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="#D9D9D9" />

    <TextView
        android:id="@+id/fill"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="86dp"
        android:layout_marginTop="218dp"
        android:textColor="#000000"
        android:textSize="20sp"
        android:text="@string/pilsu"
        android:gravity="center_vertical" />

    <ImageButton
        android:id="@+id/terms_ch"
        android:layout_width="39dp"
        android:layout_height="39dp"
        android:layout_marginTop="214dp"
        android:layout_marginStart="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/baseline_check_circle_24"
        android:contentDescription="TODO"/>

    <View
        android:id="@+id/rectangle_5"
        android:layout_width="328dp"
        android:layout_height="58dp"
        android:layout_marginTop="304dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="#D9D9D9" />

    <TextView
        android:id="@+id/fill2"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="86dp"
        android:layout_marginTop="318dp"
        android:textColor="#000000"
        android:textSize="20sp"
        android:text="@string/pre"
        android:gravity="center_vertical" />

    <ImageButton
        android:id="@+id/terms_ch2"
        android:layout_width="39dp"
        android:layout_height="39dp"
        android:layout_marginTop="313dp"
        android:layout_marginStart="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/baseline_check_circle_24"
        android:contentDescription="TODO" />

    <Button
        android:id="@+id/email_ok2"
        android:layout_width="328dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="460dp"
        android:background="@drawable/rectangle_4"
        android:text="@string/ok"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>