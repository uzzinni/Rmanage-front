<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Registerphone2">
    <TextView
        android:id="@+id/phone_register_main"
        android:layout_width="74dp"
        android:layout_height="24dp"
        android:text="@string/numer"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="74dp"
        android:layout_marginStart="142dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/phone_register_back2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="68dp"
        android:layout_marginStart="310dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/close"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/phone_register_no"
        android:layout_width="212dp"
        android:layout_height="24dp"
        android:text="@string/input"
        android:textSize="20sp"
        android:lineHeight="24.2dp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="167dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edit_phone2"
        android:layout_width="285dp"
        android:layout_height="22dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="21dp"
        android:layout_marginTop="208dp"
        android:maxLength="13"
        android:importantForAutofill="no"
        android:background="@null"
        android:inputType="phone"
        tools:ignore="LabelFor" />

    <View
        android:id="@+id/line_177"
        android:layout_width="285dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="21dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/line_177"
        />

    <Button
        android:id="@+id/register_btn_ok"
        android:layout_width="328dp"
        android:layout_height="45dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="297dp"
        android:text="@string/ok"
        android:textSize="20sp"
        android:backgroundTint="#FBEFFF"
        android:background="@drawable/rectangle_01"/>

</androidx.constraintlayout.widget.ConstraintLayout>