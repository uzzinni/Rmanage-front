<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Firstpage">

    <ImageView
        android:id="@+id/firstpage_image2"
        android:layout_width="57dp"
        android:layout_height="54dp"
        android:layout_marginTop="143dp"
        android:layout_marginStart="149dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/layer_1"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/firstpage_image"
        android:layout_width="59dp"
        android:layout_height="69dp"
        android:layout_marginTop="201dp"
        android:layout_marginStart="147dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/layer_2"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/firstpage_text"
        android:layout_width="146dp"
        android:layout_height="22dp"
        android:layout_marginTop="283dp"
        android:layout_marginStart="107dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/image3"
        android:contentDescription="TODO" />

    <Button
        android:id="@+id/firstpage_login"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="337dp"
        android:background="@drawable/rectangle_4"
        android:text="@string/login"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/firstpage_member"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="417dp"
        android:background="@drawable/rectangle_4"
        android:text="@string/member"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/first_find"
        android:layout_width="83dp"
        android:layout_height="wrap_content"
        android:text="@string/findpass"
        android:textSize="14sp"
        android:textColor="#000000"
        android:includeFontPadding="false"
        android:layout_marginTop="478dp"
        android:layout_marginStart="138dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>