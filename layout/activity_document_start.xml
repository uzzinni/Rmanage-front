<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="360px"
    android:layout_height="800px"
    tools:context=".DocumentStart">

    <TextView
        android:layout_width="98px"
        android:layout_height="24px"
        android:text="근무지 장소"
        android:textSize="20px"
        android:gravity="center"
        android:includeFontPadding="false"
        android:textColor="@color/black"
        android:layout_marginTop="74px"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/document_workplace_recycler"
        android:layout_width="328px"
        android:layout_height="500px"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="145px"
        tools:listitem="@layout/document_start_item"/>



</androidx.constraintlayout.widget.ConstraintLayout>